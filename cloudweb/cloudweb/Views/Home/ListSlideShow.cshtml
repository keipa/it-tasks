@model IEnumerable<cloudweb.Models.SlideShow>

@{
    ViewBag.Title = "You'r SlideShows";
}

<h2>Все Слайдшоу</h2>
<table class="table table-hover" align="right">

    <tr>
        <th> Название </th>
        <th>  </th>
        <th>  </th>
    </tr>

    @foreach (var item in Model)
    {

        <tr>
            <th>  <a href="@Url.Action("SlideShow", "Home", new { slide_id = item.Id})">@item.SlideName</a></th>
            <th>  <a href="@Url.Action("ChangeShow", "Home", new { slide_id = item.Id})" class="btn btn-warning">Изменить</a></th>
            <th>  <a href="@Url.Action("DeleteShow", "Home", new { slide_id = item.Id})" class="btn btn-danger">Удалить</a></th>
        </tr>
    }
</table>
